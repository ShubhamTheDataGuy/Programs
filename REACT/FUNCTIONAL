import React from 'react';

function Greeting(props) {
    return (
        <div>
            <h2>Hello, {props.name}!</h2>
            <p>Welcome to the {props.course} course.</p>
        </div>
    );
}

export default Greeting;



import React from 'react';
import Greeting from './Greeting';

function App() {
    return (
        <div>
            <Greeting name="Shubham" course="react" />
        </div>
    );
}

export default App;
